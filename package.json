{
  "name": "notion",
  "version": "1.0.0",
  "description": "base script to run server and frontend app.",
  "scripts": {
    "client": "npm --prefix ./editor start" ,
    "server": "npm --prefix ./server server:watch",
    "client-install": "npm install --prefix client",
    "client-run": "npm run start --prefix client",
    "app-run-all": "concurrently \"npm run server\" \"npm run client\""
  },
